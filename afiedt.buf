SELECT c.custName
				FROM allCustAirTickets a, Customer c
				WHERE c.cust_ID=a.custID AND
						NOT EXISTS (EXCEPT(SELECT DISTINCT ahi.airline_code
											FROM Airline_Headquartered_In ahi, SELECT DISTINCT a2.airline_code
											FROM allCustAirTickets a2
											WHERE a.custID=a2.custID
											))
/
